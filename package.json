{
  "name": "Awe",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "clear && react-native run-android",
    "ios": "clear && react-native run-ios --simulator='iPhone 15'",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "npx patch-package",
    "android-clean": "clear && cd android && ./gradlew clean && cd .. && npm run android",
    "ios-clean": "clear && cd ios && rm -rf build && xcodebuild clean && pod update && cd .. && npm run ios",
    "pod-install": "clear && cd ios && pod install && cd .."
  },
  "dependencies": {
    "react": "18.3.1",
    "react-native": "0.76.1",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-toast-message": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.0",
    "@react-native-community/cli-platform-android": "15.0.0",
    "@react-native-community/cli-platform-ios": "15.0.0",
    "@react-native/babel-preset": "0.76.1",
    "@react-native/eslint-config": "0.76.1",
    "@react-native/metro-config": "0.76.1",
    "@react-native/typescript-config": "0.76.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
